{
  "name": "sushi",
  "description": "This is an app that will help you easily calculate the cost of your sushi meals at a conveyor belt sushi restaurant.",
  "repository": {
    "type": "git",
    "url": "https://github.com/chatreejs/sushi.git"
  },
  "version": "0.1.0",
  "authors": [
    {
      "name": "Chanon Treemeth",
      "email": "chanon.t@chatree.dev"
    }
  ],
  "private": false,
  "license": "AGPL-3.0-only",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --noemit",
    "eslint": "eslint",
    "lint": "npm run eslint; npm run check-types",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:write": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@fontsource/dela-gothic-one": "^5.2.7",
    "@fontsource/varela-round": "^5.2.8",
    "antd": "^6.0.0",
    "dayjs": "^1.11.19",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@types/node": "^22.17.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
